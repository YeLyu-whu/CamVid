   "source": [
    "def filter_for_images(fileList):\n",
    "    image_list = []\n",
    "    for i in range(len(fileList)):\n",
    "        fileName = fileList[i]\n",
    "        if isImageFile(fileName):\n",
    "            image_list.append(fileName)\n",
    "    return image_list"
   ]
  },
