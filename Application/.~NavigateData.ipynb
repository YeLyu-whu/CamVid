    "def isImageFile(filePath):\n",
    "    basename_ext = os.path.splitext(filePath)\n",
    "    ext= basename_ext[1]\n",
    "    ext = ext.lower()\n",
    "    if ext=='.bmp': \n",
    "        return True\n",
    "    if ext=='.png': \n",
    "        return True\n",
    "    if ext=='.jpg': \n",
    "        return True\n",
    "    return False"
   ]
  },
