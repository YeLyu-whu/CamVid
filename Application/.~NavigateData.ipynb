    "def pase_Label_File(filePath):\n",
    "    f = open(filePath,mode='r')\n",
    "    lines = f.readlines()\n",
    "    label_items=[]\n",
    "    for i in range(len(lines)):\n",
    "        item = lines[i]\n",
    "        item = item.split()\n",
    "        r = np.uint8(item[0])\n",
    "        g = np.uint8(item[1])\n",
    "        b = np.uint8(item[2])\n",
    "        name = (item[3])\n",
    "        print((r,g,b,name))\n",
    "        label_items.append((r,g,b,name))\n",
    "    f.close()\n",
    "    return label_items"
   ]
  },
